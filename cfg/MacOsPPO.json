{
    "sSize": [1, 120],
    "aSize":2,
    "envName":"./Env/MacOs/Env01",
  
    "env":{
      "imgMode":false,
      "resolution":3,
      "nAgent": 3,
      "maxStack":20,
      "Count": 1000,
      "coeffAngV":0,
      "coeffMAngV":0,
      "coeffDDist":-0,
      "coeffVelo":0,
      "coeffTDist":0.5,
      "refVelo":2,
      "endReward":0,
      "rewardOffset":-0.01,
      "bumpPenalty":-0.5,
      "collisionPenalty":-2,
      "objRewardN":8,
      "nUpdateDist":4,
      "detectDistance":10
    },
    
    "agent": {
      "actor":{
        "module01":{
          "netCat":"MLP",
          "iSize":127,
          "nLayer":3,
          "fSize": [256, 256, 2],
          "act":["relu", "relu", "linear"],
          "BN":false,
          "input":[0],
          "output":true
        }
      },
  
      "critic":{
        "module01":{
          "netCat":"MLP",
          "iSize":127,
          "nLayer":3,
          "fSize": [256, 256, 2],
          "act":["relu", "relu", "linear"],
          "BN":false,
          "input":[0],
          "output":true
        }
      }
    },
  
    "gamma": 0.99,
    "lambda":0.995,
    "rScaling": 1,
    "entropyCoeff":0,
    "epsilon":0.2,
    "updateStep":160,
    "div":1,
    "epoch":80,
    "updateOldP":1,
    "initLogStd":0,
    "finLogStd":-2,
    "annealingStep":1e6,
  
    "optim": {
      "actor": {
        "name": "adam",
        "lr": 3e-4,
        "decay": 0,
        "eps": 1e-7,
        "clipping": false
      },
      "critic": {
        "name": "adam",
        "lr": 3e-4,
        "decay": 0,
        "clipping": false,
        "eps": 1e-7
      }
    },
    
  
    "sPath": "./save/PPO/",
    "writeTMode":true,
    "tPath": "./tensorboard/PPO/",
    "lPath":"None",
  
    "device": "cpu",
    "gpuOverload":false,
    "inferMode": false,
    "renderMode": true
  }