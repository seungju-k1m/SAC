{
  "sSize": [1, 120],
  "aSize":2,
  "envName":"./Env/Linux/simEnv01",

  "env":{
    "imgMode":false,
    "resolution":3,
    "nAgent": 3,
    "maxStack":4,
    "Count": 1000,
    "coeffAngV":-0.2,
    "coeffMAngV":-0,
    "coeffDDist":2.2,
    "endReward":0,
    "objRewardN":8,
    "nUpdateDist":1,
    "coeffInnerProduct":0,
    "coeffIntegral":0
  },
  
  "agent": {
    "actor":{
      "module01":{
        "netCat": "MLP",
        "nLayer": 3,
        "fSize": [256, 256, 4],
        "act": ["relu", "relu", "linear"],
        "BN": false,
        "iSize":127,
        "output":true,
        "input":[0]
      }
    },

    "critic": {
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },

      "module02":{
        "netCat": "MLP",
        "nLayer": 2,
        "fSize": [256, 1],
        "act": ["relu", "linear"],
        "BN": false,
        "iSize":129,
        "output":true
      }
    }
  },

  "tau": 0.005,
  "gamma": 0.99,
  "startStep": 10000,
  "fixedTemp": false,
  "tempValue": 0.2,
  "nReplayMemory": 1e6,
  "bSize": 256,
  "lrFreq": 1,
  "rScaling": 1,

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7,
      "clipping": false
    },
    "critic": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "clipping": false,
      "eps": 1e-7
    },
    "temperature": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "clipping": false,
      "eps": 1e-7
    }
  },
  

  "sPath": "./save/SAC/",
  "writeTMode":true,
  "tPath": "./tensorboard/SAC/",
  "lPath":"None",

  "device": "cuda:0",
  "gpuOverload":false,
  "inferMode": false,
  "renderMode": true
}