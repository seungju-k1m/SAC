{
  "sSize": [1, 120],
  "aSize":2,
  "envName":"./Env/Linux/DemoEnv01",

  "env":{
    "LSTMMode":1,
    "ImgMode":false,
    "Resolution_LidarAngle":1,

    "Number_Agent": 3,
    "Number_DynamicObstacle":4,
    "Number_StaticObstacle":5,

    "Number_MaxCollision":1,
    "MaxStep": 9999,
    "Penallty_Maginitude_AngularVelocity":0,
    "RecommendedYawRate":0.7,
    "Velocity":4,
    "YawRate":1,
    "Penalty_OverAngularVelocity":0,
    "Reward_ReachingDestination":0.01,
    "Reward_ArrivingDestination":12,
    "Reward_ArrivingStartPoint":12,
    "Penalty_OverMaxStep":0,
    "Penalty_Time":0,
    "Penalty_Collision_Wall":-0.02,
    "Penalty_Collision_Agent":-0.2,
    "Penalty_Collision_DynamicOBS":-0.2,
    "Number_MaxArrivingReward":1e6,
    "IntervalStep":20,
    "LidarMeasurementDistance":10
  },
  
  "agent": {
    "actor":{
      "module00":{
        "netCat":"MLP",
        "iSize":126,
        "nLayer":3,
        "fSize":[256, 256, 2],
        "act":["relu", "relu", "linear"],
        "BN":false,
        "input":[0],
        "output":true
    }
    },

    "critic":{
        "module00":{
            "netCat":"MLP",
            "iSize":126,
            "nLayer":3,
            "fSize":[256, 256, 1],
            "act":["relu", "relu", "linear"],
            "BN":false,
            "input":[0],
            "output":true
        }
    }
  },

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 1e-6,
      "eps": 1e-7,
      "clipping": false
    },
    "critic": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 1e-6,
      "clipping": false,
      "eps": 1e-7
    }
  },

  "gamma": 0.99,
  "lambda":0.95,
  "rScaling": 1,
  "entropyCoeff":0,
  "epsilon":0.2,
  "updateStep":160,
  "div":1,
  "epoch":1,
  "updateOldP":4,
  "initLogStd":-1.1,
  "finLogStd":-1.5,
  "annealingStep":1e6,

  "sPath": "./save/PPO/",
  "writeTMode":true,
  "tPath": "./tensorboard/PPO/",
  "lPath":"None",

  "device": "cpu",
  "gpuOverload":false,
  "inferMode": false,
  "renderMode": true
}