{
  "sSize": [1, 120],
  "aSize":2,
  "envName":"./Env/Linux/Env24",
  "LSTMNum":-1,

  "env":{
    "ImgMode":false,
    "Resolution_LidarAngle":3,
    "Number_Agent": 72,
    "Number_MaxCollision":200,
    "MaxStep": 1000,
    "Penallty_Maginitude_AngularVelocity":-0.1,
    "RecommendedYawRate":0.7,
    "Velocity":4,
    "YawRate":1.5,
    "Penalty_OverAngularVelocity":0,
    "Reward_ReachingDestination":0.5,
    "Penalty_OverMaxStep":0,
    "Penalty_Time":-0.05,
    "Penalty_Collision_Wall":-0.5,
    "Penalty_Collision_Agent":-0.5,
    "Number_MaxArrivingReward":8,
    "IntervalStep":4,
    "LidarMeasurementDistance":10
  },
  
  "agent": {
    "actor":{
      "module01":{
        "netCat":"MLP",
        "iSize":126,
        "nLayer":3,
        "fSize": [256, 256, 2],
        "act":["relu", "relu", "linear"],
        "BN":false,
        "input":[0],
        "output":true
      }
    },

    "critic":{
      "module01":{
        "netCat":"MLP",
        "iSize":126,
        "nLayer":3,
        "fSize": [256, 256, 1],
        "act":["relu", "relu", "linear"],
        "BN":false,
        "input":[0],
        "output":true
      }
    }
  },

  "gamma": 0.99,
  "lambda":0.995,
  "rScaling": 1,
  "entropyCoeff":0,
  "epsilon":0.2,
  "updateStep":128,
  "div":1,
  "epoch":15,
  "updateOldP":1,
  "initLogStd":-0.7,
  "finLogStd":-1.6,
  "annealingStep":1e6,

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7,
      "clipping": false
    },
    "critic": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "clipping": false,
      "eps": 1e-7
    }
  },
  

  "sPath": "./save/PPO/",
  "writeTMode":true,
  "tPath": "./tensorboard/PPO/",
  "lPath":"None",

  "device": "cuda:0",
  "gpuOverload":false,
  "inferMode": false,
  "renderMode": true
}