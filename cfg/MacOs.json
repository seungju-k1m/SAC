{
  "envName":"./Env/MacOs/Env16",
  "sSize": [1, 120],
  "resolution":3,
  "nAgent": 48,
  "maxStack":1,
  "imgMode":"False",
  "aSize": 2,
  "seed": 0,
  "fixedSigma":"True",

  "agent": {
            "CNN1D": {
              "netCat": "CNN1D",
              "nLayer": 2,
              "nUnit": [8, 16],
              "stride": [2, 2],
              "fSize" : [3, 3,-1],
              "padding" : [1, 1],
              "act": "relu",
              "BN": "None",
              "linear":"True"
            },
            "LSTM": {
              "netCat": "LSTMNET",
              "nLayer": 1,
              "hiddenSize":256
            },
            "actor":{
                "netCat":"MLP",
                "nLayer":2,
                "fSize":[256, 2],
                "act":["relu", "linear"],
                "BN":"None"
            },

            "critic":{
                "netCat":"MLP",
                "nLayer":2,
                "fSize":[256, 1],
                "act":["relu", "linear"],
                "BN":"None"
            }
  },
  "updateStep":10,
  "div":2,

  "lrFreq": 1,
  "rScaling": 1,

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-5,
      "decay": 0,
      "eps": 1e-7,
      "clipping": "None"
    },
    "critic": {
      "name": "adam",
      "lr": 3e-5,
      "decay": 0,
      "clipping": "None",
      "eps": 1e-7
    }
  },
  "gamma": 0.99,
  "Lambda":0.95,
  "epoch":80,
  "epsilon": 0.2,
  "entropyCoeff":0.01,
  "initLogStd": -0.7,
  "finLogStd":-2.5,
  "annealingStep":5e5,

  "sPath": "./save/PPO/",
  "writeTMode":"False",
  "tPath": "./tensorboard/PPO/",
  "lPath":"None",

  "device": "cpu",
  "gpuOverload":"True",
  "inferMode": "False",
  "renderMode": "True"
}