{
    "envName":"./Env/MacOs/simEnv16",
    "sSize": [1, 726],
    "nAgent": 48,
    "imgMode":"False",
    "aSize": 2,
    "seed": 0,
  
    "agent": {
              "Feature": {
                "netCat": "LSTMNET",
                "nLayer": 1,
                "hiddenSize":512
              },
              "actor":{
                  "netCat":"MLP",
                  "nLayer":2,
                  "fSize":[256, 4],
                  "act":["relu", "linear"],
                  "BN":"None"
              },
  
              "critic":{
                  "netCat":"MLP",
                  "nLayer":2,
                  "fSize":[256, 1],
                  "act":["relu", "linear"],
                  "BN":"None"
              }
    },
    "episodeP": 20,
    "updateStep":200,
  
