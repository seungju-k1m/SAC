{
  "sSize": [1, 120],
  "aSize":2,
  "envName":"./Env/MacOs/Env01",
  "ICMMode":false,

  "env":{
    "ImgMode":false,
    "IntervalStep":4,
    "LidarMeasurementDistance":10,
    "Resolution_LidarAngle":3,
    "RecommendedYawRate":0.7,
    "Velocity":3,
    "YawRate":1,
    
    "MaxStep": 3000,
    "Size_Floor":1,

    "Number_Agent": 3,
    "Number_MaxCollision":20,
    "Number_MaxArrivingReward":8,
    "Number_DynamicObstacle":3,
    "Number_StaticObstacle":7,
    

    "Penalty_OverAngularVelocity":0,
    "Penalty_OverMaxStep":0,
    "Penalty_Time":-0.01,
    "Penalty_Collision_Wall":-0.5,
    "Penalty_Collision_Agent":-2,
    "Penallty_Maginitude_AngularVelocity":0,

    "Reward_ReachingDestination":0.3

  },
  
  
  "agent": {
    "actor":{
      "module01":{
        "netCat": "MLP",
        "nLayer": 3,
        "fSize": [256, 256, 4],
        "act": ["relu", "relu", "linear"],
        "BN": false,
        "iSize":126,
        "output":true,
        "input":[0]
      }
    },

    "critic": {
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },

      "module02":{
        "netCat": "MLP",
        "nLayer": 3,
        "fSize": [256, 256, 1],
        "act": ["relu", "relu", "linear"],
        "BN": false,
        "iSize":128,
        "output":true
      }
    },
    "Forward":{
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },
      "module02":{
        "netCat":"MLP",
        "nLayer":2,
        "act":["relu", "linear"],
        "fSize":[256, 256],
        "BN":false,
        "iSize":258,
        "output":true
      }
    },
    "Feature":{
      "module01":{
        "netCat":"MLP",
        "nLayer":2,
        "act":["relu", "linear"],
        "fSize":[256, 256],
        "BN":false,
        "iSize":127,
        "input":[0],
        "output":true
      }
    },
    "inverseModel":{
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },
      "module02":{
        "netCat":"MLP",
        "nLayer":2,
        "act":["relu", "linear"],
        "fSize":[256, 2],
        "BN":false,
        "iSize":512,
        "output":true
      }
    }
  },

  "tau": 0.005,
  "gamma": 0.99,
  "startStep": 10000,
  "fixedTemp": true,
  "tempValue": 0.005,
  "nReplayMemory": 1e6,
  "bSize": 256,
  "lrFreq": 1,
  "rScaling": 1,

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7
    },
    "critic": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7
    },
    "temperature": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7
    }
  },

  "sPath": "./save/SAC/",
  "writeTMode":true,
  "tPath": "./tensorboard/SAC/",
  "lPath":"None",

  "device": "cpu",
  "gpuOverload":false,
  "inferMode": false,
  "renderMode": true
}