{
  "sSize": [1, 120],
  "aSize":2,
  "envName":"./Env/MacOs/Env01",
  "ICMMode":false,

  "env":{
    "imgMode":false,
    "resolution":3,
    "nAgent": 3,
    "maxStack":20,
    "Count": 1000,
    "coeffAngV":0,
    "coeffMAngV":0,
    "coeffDDist":-0,
    "coeffVelo":0,
    "coeffTDist":0.5,
    "refVelo":2,
    "endReward":0,
    "rewardOffset":-0.01,
    "bumpPenalty":-0.5,
    "collisionPenalty":-2,
    "objRewardN":8,
    "nUpdateDist":4,
    "detectDistance":10
  },
  
  "agent": {
    "actor":{
      "module01":{
        "netCat": "MLP",
        "nLayer": 3,
        "fSize": [256, 256, 4],
        "act": ["relu", "relu", "linear"],
        "BN": false,
        "iSize":127,
        "output":true,
        "input":[0]
      }
    },

    "critic": {
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },

      "module02":{
        "netCat": "MLP",
        "nLayer": 3,
        "fSize": [256, 256, 1],
        "act": ["relu", "relu", "linear"],
        "BN": false,
        "iSize":129,
        "output":true
      }
    },
    "Forward":{
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },
      "module02":{
        "netCat":"MLP",
        "nLayer":2,
        "act":["relu", "linear"],
        "fSize":[256, 256],
        "BN":false,
        "iSize":258,
        "output":true
      }
    },
    "Feature":{
      "module01":{
        "netCat":"MLP",
        "nLayer":2,
        "act":["relu", "linear"],
        "fSize":[256, 256],
        "BN":false,
        "iSize":127,
        "input":[0],
        "output":true
      }
    },
    "inverseModel":{
      "module01":{
        "netCat":"Cat",
        "input":[0, 1]
      },
      "module02":{
        "netCat":"MLP",
        "nLayer":2,
        "act":["relu", "linear"],
        "fSize":[256, 2],
        "BN":false,
        "iSize":512,
        "output":true
      }
    }
  },

  "tau": 0.005,
  "gamma": 0.99,
  "startStep": 10000,
  "fixedTemp": true,
  "tempValue": 0.005,
  "nReplayMemory": 1e6,
  "bSize": 256,
  "lrFreq": 1,
  "rScaling": 1,

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7
    },
    "critic": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7
    },
    "temperature": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7
    }
  },

  "sPath": "./save/SAC/",
  "writeTMode":true,
  "tPath": "./tensorboard/SAC/",
  "lPath":"None",

  "device": "cpu",
  "gpuOverload":false,
  "inferMode": false,
  "renderMode": true
}