{
    "envName":"./Env/MacOs/simEnv01",
    "unityEnv": "True",
    "imgMode":"True",
    "sSize": [1, 192, 192],
    "nAgent": 5,
    "aSize": 2,
    "seed": 0,
  
    "agent": {
      "actorFeature": {
        "netCat": "CNET",
        "nLayer": 5,
        "nUnit": [8, 16, 32, 64],
        "stride": [2, 2, 2, 2],
        "fSize" : [3, 3, 3, 3, -1],
        "padding" : [1, 1, 1, 1],
        "act": "relu",
        "BN": "None"
      },

      "actor":{
        "netCat": "MLP",
        "nLayer": 2,
        "fSize": [256, 4],
        "act": ["relu", "linear"],
        "BN": "None"
      },

      "criticFeature": {
        "netCat": "CNET",
        "nLayer": 5,
        "nUnit": [8, 16, 32, 64],
        "stride": [2, 2, 2, 2],
        "padding": [1, 1, 1, 1],
        "fSize" : [3, 3, 3, 3, -1],
        "act": "relu",
        "BN": "None"
      },

      "critic": {
        "netCat": "MLP",
        "nLayer": 2,
        "fSize":[256, 1],
        "act": ["relu", "linear"],
        "BN":"None"
      }
    },
  
    "tau": 0.005,
    "sMode": "True",
    "startStep": 1000,
  
    "fixedTemp": "True",
    "tempValue": 0.2,
  
    "nReplayMemory": 1e6,
    "bSize": 256,
    "lrFreq": 1,
    "rScaling": 1,
  
    "optim": {
      "actor": {
        "name": "adam",
        "lr": 3e-4,
        "decay": 0,
        "eps": 1e-7,
        "clipping": "None"
      },
      "critic": {
        "name": "adam",
        "lr": 3e-4,
        "decay": 0,
        "clipping": "None",
        "eps": 1e-7
      }
    },
    "gamma": 0.99,
  
    "sPath": "./save/SAC/",
    "writeTMode":"False",
    "tPath": "./tensorboard/SAC/",
    "lPath":"None",
  
    "device": "cpu",
    "inferMode": "False",
    "renderMode": "True"
  }