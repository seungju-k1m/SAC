{
  "envName":"./Env/MacOs/Env01",
  "sSize": [1, 127],
  "nAgent": 3,
  "maxStack":1,
  "imgMode":"False",
  "resolution":3,
  "aSize": 2,
  "seed": 0,
  "fixedSigma":"False",

  "agent": {
    "Module01": {
      "netCat":"CNN1D",
      "nLayer":1,
      "nUnit": [8],
      "stride": [2],
      "fSize": [7],
      "padding": [1],
      "act": ["relu"],
      "input":"True"
    },
    "Module02": {
      "netCat":"Residul1D"
    },
    "Module03": {
      "netCat": "Flatten"
    },
    "Module04": {
      "netCat": "Cat",
      "count":2,
      "iSize":[-1, 6],
      "input": "True"
    },
    "Module05": {
      "netCat": "LSTMNET",
      "hiddenSize": 256
    },
    "Module06": {
      "netCat": "MLP",
      "nLayer": 2,
      "fSize": [256, 4],
      "act": ["relu", "linear"],
      "branch":"True"
    },
    "Module07": {
      "neCat": "MLP",
      "nLayer":2,
      "fSize":[256, 1],
      "act":["relu", "linear"],
      "branch":"True"
    }
  },
  "updateStep":256,
  "div":1,

  "lrFreq": 1,
  "rScaling": 1,

  "optim": {
    "Module": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7,
      "clipping": "None"
    }
  },
  "gamma": 0.99,
  "Lambda":0.95,
  "epoch":80,
  "epsilon": 0.2,
  "entropyCoeff":0.01,
  "initLogStd": -0.7,
  "finLogStd":-2.5,
  "annealingStep":5e5,

  "sPath": "./save/PPO/",
  "writeTMode":"True",
  "tPath": "./tensorboard/PPO/",
  "lPath":"None",

  "device": "cpu",
  "gpuOverload":"True",
  "inferMode": "False",
  "renderMode": "True"
}