{
  "envName":"./Env/Linux/Env16",
  "sSize": [1, 96, 96],
  "nAgent": 48,
  "imgMode":"True",
  "aSize": 2,
  "seed": 0,

  "agent": {
            "CNN": {
              "netCat": "CNET",
              "nLayer": 5,
              "nUnit": [8, 16, 32, 64],
              "stride": [2, 2, 2, 2],
              "fSize" : [3, 3, 3, 3, -1],
              "padding" : [1, 1, 1, 1],
              "act": "relu",
              "BN": "None"
            },
            "LSTM": {
              "netCat": "LSTMNET",
              "nLayer": 1,
              "hiddenSize":512
            },
            "actor":{
                "netCat":"MLP",
                "nLayer":2,
                "fSize":[256, 2],
                "act":["relu", "linear"],
                "BN":"None"
            },

            "critic":{
                "netCat":"MLP",
                "nLayer":2,
                "fSize":[256, 1],
                "act":["relu", "linear"],
                "BN":"None"
            }
  },
  "updateStep":256,
  "div":16,

  "lrFreq": 1,
  "rScaling": 1,

  "optim": {
    "actor": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "eps": 1e-7,
      "clipping": "None"
    },
    "critic": {
      "name": "adam",
      "lr": 3e-4,
      "decay": 0,
      "clipping": "None",
      "eps": 1e-7
    }
  },
  "gamma": 0.99,
  "Lambda":0.95,
  "epoch":20,
  "epsilon": 0.2,
  "entropyCoeff":0,
  "initLogStd": -0.7,
  "finLogStd":-2.5,
  "annealingStep":5e5,

  "sPath": "./save/PPO/",
  "writeTMode":"True",
  "tPath": "./tensorboard/PPO/",
  "lPath":"None",

  "device": "cuda:0",
  "gpuOverload":"True",
  "inferMode": "False",
  "renderMode": "True"
}